{
  "name": "portafolio-web-eduardo-rangel",
  "private": true,
  "version": "2.7.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docs": "typedoc --out docs src/**",
    "init-proyect": "npm install && npm run dev",
    "end-proyect": "npm run build && npm run docs",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf docs",
    "clean-windows": "rd /s /q node_modules && rd /s /q dist && rd /s /q docs"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "jsdom": "^21.0.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "three": "^0.148.0",
    "universal-cookie": "^4.0.4",
    "vitest": "^0.27.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/three": "^0.148.0",
    "@vitejs/plugin-react": "^3.0.0",
    "prismjs": "^1.29.0",
    "sass": "^1.57.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
